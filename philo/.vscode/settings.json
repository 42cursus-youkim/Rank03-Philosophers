{
  "files.associations": {
    "**/*.pug": "jade",
    "**/*.h": "c",
    "**/*.html": "html",
    "**/templates/**/*.html": "django-html",
    "**/templates/**/*.css": "css",
    "**/requirements{/**,*}.{txt,in}": "pip-requirements",
    "*.html": "html",
    "*.xpm": "c",
    "*.h": "c",
    "*.njk": "nunjucks",
    "**/templates/**/*": "django-txt",
    "atomic": "c",
    "*.tcc": "c",
    "memory": "c",
    "streambuf": "c",
    "typeinfo": "c",
    "utility": "c"
  }
}
